		margin-left 10px
		
		span
			margin-left 5px
			font-size 13px
			
		.date-hrs
			font-weight 900
		.icon-phone
			padding 8px
			border-radius 50%
			background #fff
			color #f37800
			transition background .7s
			transition color .7s
			font-size 18px

		.icon-bubble
			padding 8px
			border-radius 50%
			background #fff
			color #f37800
			transition background .7s
			transition color .7s
			font-size 18px
		.icon-clock
			padding 8px
			border-radius 50%
			background #fff
			color #f37800
			transition background .7s
			transition color .7s
			font-size 18px

body
	margin 0
	
ul 
	list-style none
a
	text-decoration none

.header-top
	display block
	background #eaeaea
	hight 49px
	/a 
		color #838383
		
	section
		display inline-block
		/li
			display inline-block
	

.header-top-left
	margin-left 80px
	li
		margin-left 5px
		span
			margin-left 5px	
		.icon-drawer
			padding 8px
			transition background .2s
			border-radius 50%
		.icon-users
			padding 8px
			transition background .2s
			border-radius 50%
	

.header-top-right
	float right
	margin-right 100px
	li
		margin-left 10px
		span
			margin-left 5px
		.icon-phone
			padding 8px
			border-radius 50%
			background #fff
			color #f37800
			transition background .3s
		.icon-bubble
			padding 8px
			border-radius 50%
			background #fff
			color #f37800
			transition background .3s
		.icon-clock
			padding 8px
			border-radius 50%
			background #fff
			color #f37800
			transition background .3s
		.icon-search
			padding 8px
			border-radius 50%
			background #f37800
			color #fff
			border 1px solid
			border-color #f37800
			transition background .3s
		.icon-facebook
			border 1px solid
			border-radius 50%
			padding 5px
			font-size 20px
			transition background .3s
		.icon-twitter
			border 1px solid
			border-radius 50%
			padding 5px
			font-size 20px
			transition background .3s
		.icon-google
			border 1px solid
			border-radius 50%
			padding 5px
			font-size 20px
			transition background .3s
		.icon-youtube
			border 1px solid
			border-radius 50%
			padding 5px
			font-size 20px
			transition background .3s
			

		
				
			
	.social-icon
		margin 0
		




.header-bottom
	display inline-block
	width 100%
	section
		display inline-block
	
.header-bottom-left
	margin 30px 0 20px 70px
	

.header-bottom-right
	float right
	margin 20px 100px 0 0
	
	li
		padding 10px 20px
		font-size 17px
		a
			color black
	.border-nav
		border-color #eaeaea
		border-right 1px solid
		



.logo-image
	width 269px
	hight 48px


	
